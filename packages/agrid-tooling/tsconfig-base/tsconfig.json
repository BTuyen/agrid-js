{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ESNext",
        "lib": ["ESNext", "DOM"],
        "strict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noUnusedParameters": true
    }
}
